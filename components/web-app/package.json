{
  "name": "solid-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkeBonn/solid-ui.git"
  },
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/solid-ui.umd.cjs",
  "module": "./dist/solid-ui.js",
  "exports": {
    ".": {
      "import": "./dist/solid-ui.js",
      "require": "./dist/solid-ui.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "./node_modules/.bin/vite",
    "build": "./node_modules/.bin/vite build && vue-tsc --emitDeclarationOnly",
    "preview": "./node_modules/.bin/vite preview",
    "storybook": "./node_modules/.bin/storybook dev -p 6006 --ci",
    "build-storybook": "./node_modules/.bin/storybook build"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/vue3": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/tsconfig": "^0.7.0",
    "playwright": "^1.51.1",
    "storybook": "^8.6.12",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.4"
  },
  "license": "MIT"
}
